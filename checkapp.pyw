def clean(str_list):
    data_cleaned = []
    for each in str_list:
        str_fix = re.sub(r'null', '\'\'', each)
        str_dic = eval(str_fix)
        data_cleaned.append(str_dic['rows'])

    return data_cleaned


def flow_statastic(datalist, filename):
    df_list = []
    for each in datalist:
        df_each = pd.DataFrame(each)
        df_list.append(df_each)

    df = pd.concat(df_list)
    time_ = '流向' + time.strftime('%Y%m%d %H%M%S', time.localtime())

    df['createtime'] = pd.to_datetime(df['createtime'])
    df['year'] = pd.DatetimeIndex(df['createtime']).year
    df['month'] = pd.DatetimeIndex(df['createtime']).month
    df['month'] = df['month'].map(str)
    df['month'] = df['month'].map(lambda x: x if len(x) == 2 else '0' + x)
    df['日期'] = df['year'].map(str) + '-' + df['month']

    summary = df.pivot_table(['itemQty'], index=['customerName'], columns=['日期', 'itemName', 'billtypename'],
                             aggfunc=sum, fill_value=0)
    if not os.path.exists('.\\{}'.format(filename)):
        os.makedirs('.\\{}'.format(filename))
    writer = pd.ExcelWriter('.\\{}\\{}.xlsx'.format(filename, time_))
    df.to_excel(writer, sheet_name='明细', index=False)
    summary.to_excel(writer, sheet_name='统计')
    writer.save()
    os.startfile('.\\{}\\{}.xlsx'.format(filename, time_))


def stock_statastic(datalist, filename):
    df_list = []
    for each in datalist:
        df_each = pd.DataFrame(each)
        df_list.append(df_each)

    df = pd.concat(df_list)

    df['productDate'] = df['productDate'].str.replace('00:00:00', '')
    df['overdueDate'] = df['overdueDate'].str.replace('00:00:00', '')
    df['效期'] = df['productDate'] + df['overdueDate']
    summary = df.pivot_table(['itemQty'], index=['效期'], columns=['itemName'], aggfunc=sum,
                             fill_value=0)
    if not os.path.exists('.\\{}'.format(filename)):
        os.makedirs('.\\{}'.format(filename))
    time_ = '库存' + time.strftime('%Y%m%d %H%M%S', time.localtime())
    writer = pd.ExcelWriter('.\\{}\\{}.xlsx'.format(filename, time_))
    df.to_excel(writer, sheet_name='明细', index=False)
    summary.to_excel(writer, sheet_name='统计')
    writer.save()
    os.startfile('.\\{}\\{}.xlsx'.format(filename, time_))


